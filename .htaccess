RewriteEngine On

RewriteRule ^post$ /index.php?page=post [L]
# RewriteRule ^post/([a-zA-Z0-9-]+)$ /index.php?page=single&post=$1 [L]
RewriteRule ^post/([0-9]+)$ /index.php?page=single&post=$1 [L]
RewriteRule ^tags/([0-9]+)$ /index.php?page=tags&tag=$1 [L]
RewriteRule ^accueil$ /index.php?page=accueil [L]

